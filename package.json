    "postcss-mixins": "^6.2.0",
    "postcss-simple-vars": "^5.0.2",
    "pre-commit": "^1.2.2",
    "react": "^16.4.0",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.4.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^4.0.0",
    "run-with-testrpc": "^0.3.0",
    "storybook-host": "^5.0.3",
    "storybook-router": "^0.3.3",
    "style-loader": "^0.23.1",
    "truffle": "^5.0.6",
    "truffle-contract": "^4.0.7",
