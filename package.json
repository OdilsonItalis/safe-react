    "webpack-dev-server": "^3.1.0",
    "webpack-manifest-plugin": "^2.0.0-rc.2"
  },
  "dependencies": {
    "@gnosis.pm/util-contracts": "^2.0.0",
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "connected-react-router": "^6.3.1",
    "final-form": "^4.2.1",
    "history": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-final-form": "^4.1.0",
    "react-loadable": "^5.3.1",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.30.0",
    "web3": "1.0.0-beta.37"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/jest.setup.js"
    ],
    "setupFiles": [
      "<rootDir>/config/webpack.config.test.js",
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/LocalStorageMock.js",
      "<rootDir>/config/jest/Web3Mock.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:8000",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(css|scss)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]flow-typed[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "~(.*)$": "<rootDir>/src/$1",
      "#(.*)$": "<rootDir>/safe-contracts/build/contracts/$1",
      "^react-native$": "react-native-web"
    }
  }
}
